<!DOCTYPE html>
<html lang="en">
<head>
    {% set cache_buster = "" | currentTime %}
    <meta charset="UTF-8">
    <title>{{ currentTime }}</title>
    <meta name="viewport" content="width=device-width">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link rel="manifest" href="/manifest.json?cb={{cache_buster}}">
    {% block cdn %}
        <script src="https://cdn.jsdelivr.net/npm/clipboard@2.0.8/dist/clipboard.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/sortablejs@1.15.0/Sortable.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/axios@0.27.2/dist/axios.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.socket.io/4.0.0/socket.io.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/qrcode@1.5.1/build/qrcode.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/animejs@3.2.1/lib/anime.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
    {% endblock %}
    {% for key, value in 'public_routes' | getParam %}
        <meta name="{{ key }}" content="{{ value }}">
    {% endfor %}
    {% block local %}
        <script src="/script/get_url.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/settings.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/generate_qr_code.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/copy_link.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/generic_dialog.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/signal.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/dashboard.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/socket.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/scroll_snap.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/set_css_variable.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/iframe.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/modules.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/press_hold.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/debounce.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="/script/device_type.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <link href="/style/font.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="/style/reset.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="/style/base.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="/style/variables.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="/style/scrollbar.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="/style/generic_dialog.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="/style/controls.css?cb={{cache_buster}}" rel="stylesheet">
    {% endblock %}
    {% block hscripts %}{% endblock %}
</head>
<body>
{% block body %}{% endblock %}
{% block extras %}{% endblock %}
{% block fscripts %}{% endblock %}
</body>
</html>