<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <!-- PRELOAD -->
    {% set assetLinks = "" | generateAssetLinks %}
    {% for link in assetLinks %}<link rel="preload" as="{{ link.type }}" href="{{ link.href | safe }}">
    {% endfor %}
    <!-- CDN -->
    <script defer src="https://cdn.jsdelivr.net/npm/sortablejs@1.15.0/Sortable.min.js" type="text/javascript"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js" type="text/javascript"></script>
    <script defer src="https://cdn.socket.io/4.0.0/socket.io.min.js" type="text/javascript"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/qrcode/build/qrcode.min.js"></script>

    <!-- LOCAL -->
    <script defer src="/script/generate_qr_code.js" type="text/javascript"></script>
    <script defer src="/script/generic_dialog.js" type="text/javascript"></script>
    <script defer src="/script/send_signal.js" type="text/javascript"></script>
    <script defer src="/script/sortable_buttons.js" type="text/javascript"></script>
    <script defer src="/script/socket_connect.js" type="text/javascript"></script>
    <!-- CSS -->
    <link rel="stylesheet" href="/style/reset.css">
    <link rel="stylesheet" href="/style/font.css">
    <link rel="stylesheet" href="/style/generic_dialog.css">
    <link rel="stylesheet" href="/style/header_grid.css">

    {% block hscripts %}{% endblock %}

</head>
<body>

{% block body %}{% endblock %}
{% block fscripts %}{% endblock %}

</body>
</html>