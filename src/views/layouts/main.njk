<!DOCTYPE html>
<html lang="en">
<head>
    {%set cache_buster = "" | currentTime%}
    <meta charset="UTF-8">
    <title>{{currentTime}}</title>
    <meta name="viewport" content="width=device-width">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link rel="manifest" href="../../assets/manifest.json?cb={{cache_buster}}">
    <link rel="icon" href="../../assets/icon512_rounded.png" type="image/png">
    {%block cdn%}
        <script src="https://cdn.jsdelivr.net/npm/clipboard@2.0.8/dist/clipboard.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/sortablejs@1.15.0/Sortable.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/axios@0.27.2/dist/axios.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.socket.io/4.0.0/socket.io.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/qrcode@1.5.1/build/qrcode.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/animejs@3.2.1/lib/anime.min.js" defer crossorigin="anonymous" type="text/javascript"></script>
    {%endblock%}
    {%for key, value in 'public_routes'|getParam%}
        <meta name="{{key}}" content="{{value}}">
    {%endfor%}
    {%block local%}
        <script src="../../assets/script/css_dev_watch.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/service_worker.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/get_url.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/settings.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/generate_qr_code.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/generate_copy_link.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/send_signal.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/dashboard.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/socket.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/scroll_snap.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/set_css_variable.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/iframe_detect.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/modules.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/detect_press_hold.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <script src="../../assets/script/debounce_function.js?cb={{cache_buster}}" defer type="text/javascript"></script>
        <link href="../../assets/style/reset.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="../../assets/style/base.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="../../assets/style/variables.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="../../assets/style/scrollbar.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="../../assets/style/controls.css?cb={{cache_buster}}" rel="stylesheet">
        <link href="../../assets/style/dialog.css?cb={{cache_buster}}" rel="stylesheet">
    {%endblock%}
    {%block head%}{%endblock%}
</head>
<body>
{%block body%}{%endblock%}
{%block dialogs%}{%endblock%}
{%block fscripts%}{%endblock%}
</body>
</html>