{%set public_routes = 'public_routes'|getParam%}
<nav>
<input type="checkbox" id="toggle-nav-show">
<label for="toggle-nav-show" aria-label="🌀"></label>
<aside>
<button onclick="javascript:socketEmitReload()" aria-label="Reload"><em aria-label="🔄"></em></button>
<button onclick="javascript:document.body.querySelector('#qr-dialog').showModal()" aria-label="Show QR"><em aria-label="🔗"></em></button>
<button onclick="javascript:window.parent.location.href='{{public_routes.PANEL_DASHBOARD}}'" aria-label="Dev"><em aria-label="⚙"></em></button>
<button onclick="javascript:window.parent.location.href='{{public_routes.INDEX}}'" aria-label="App"><em aria-label="🕹️"></em></button>
<button onclick="javascript:getURL('{{public_routes.TWITCH_LOGIN}}')" aria-label="Login"><em aria-label="🔒"></em></button>
<button onclick="javascript:getURL('{{public_routes.PANEL_DRAW}}')" aria-label="Draw"><em aria-label="🖌️"></em></button>
<button onclick="javascript:getURL('{{public_routes.PANEL_MODULES}}')" aria-label="Layout"><em aria-label="🎬"></em></button>
<button onclick="javascript:getURL('{{public_routes.PANEL_SETTINGS}}')" aria-label="Actions"><em aria-label="⚡"></em></button>
<button style="display:none;" id="toggle-edit-dashboard"></button>
</aside>
</nav>