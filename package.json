{
  "main": "src/index.js",
  "scripts": {
    "start": "rm -rf .signals.json .style.css && node src/index.js",
    "preview": "node compiler/compile.js",
    "reset": "rm -rf node_modules && npm i",
    "cert": "openssl req --nodes -new -x509 -keyout localhost.key -out localhost.crt -days 365",
    "bpm": "rm -rf .bin && curl -L \"https://github.com/Rexios80/hds_desktop/releases/download/0.2.3/hds_desktop_windows.exe\" -o \"./.bin\"",
    "combine": "node compiler/combine.js",
    "uncombine": "node compiler/uncombine.js",
    "pkg": "set DEBUG=pkg* && pkg .",
    "zombies": "node compiler/zombies.js"
  },
  "name": "app-name",
  "bin": "src/index.js",
  "pkg": {
    "outputPath": "dist",
    "assets": [
      "src/client/**/*"
    ],
    "scripts": [
      "src/**/*.js",
      "node_modules/axios/**/*.js"
    ],
    "targets": [
      "node16-win-x64"
    ]
  },
  "dependencies": {
    "aws-sdk": "2.1692.0",
    "chalk": "2.4.2",
    "dotenv": "16.4.5",
    "express": "4.21.2",
    "glob": "11.0.0",
    "node-fetch": "2.6.12",
    "nunjucks": "3.2.4",
    "pkg": "5.8.1",
    "prettier": "^3.4.2",
    "socket.io": "4.8.1",
    "sync-request": "6.1.0",
    "tmi.js": "1.8.5",
    "twitter-api-v2": "1.18.2"
  }
}
